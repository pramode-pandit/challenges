
function searchkeyval
{

data=$1
skey=$2

# echo data=$data
# echo skey=$skey

for i in $(echo "$data" | jq '. | keys[]'); do

# echo "key -> $i"

# echo "$data" | jq -c ."${i}"

val=$(echo "$data" | jq -c ."${i}")

# echo "value -> "$val


if [ $i = $skey ]
then
  echo "skey found"
  echo "$skey -> $val"
fi


val_type=$(echo "[${val}]" | jq -r 'map(type)' | jq -r .[])

# echo  "type -> "${val_type}

if [ $val_type == "object" ]
then
   searchkeyval $val $skey
fi

done
}

